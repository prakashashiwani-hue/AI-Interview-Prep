{
  "name": "ai-interview-preparator",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "db:generate": "cd packages/db && npx prisma generate && cd ../..",
    "db:migrate": "cd packages/db && npx prisma migrate deploy && cd ../..",
    "db:setup": "pnpm db:generate && pnpm db:migrate",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "prettier": "^3.8.1",
    "turbo": "^2.8.1",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "better-sqlite3",
      "core-js-pure",
      "esbuild",
      "prisma",
      "sharp"
    ]
  }
}